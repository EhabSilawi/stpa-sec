              controledProcessSenders="//@systems.0/@systemModel.0/@subsystemModel.0/@systemModel.0/@controlledprocess.0"/>
          <otherinformation
              name="info 2"
              description="info 2"
              controllerSenders="//@systems.0/@systemModel.0/@subsystemModel.0/@systemModel.0/@controller.1"/>
        </systemModel>
      </subsystemModel>
      <subsystemModel
          name="Reference Aircraft**"
          description="Reference Aircraft**">
