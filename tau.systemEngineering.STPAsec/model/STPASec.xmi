            modelGoal="Reference Aircraft"
            description="Reference Aircraft"
            swndsInfor="//@systems.0/@systemModel.0/@otherinformation.4"
            receivesCommands="//@systems.0/@systemModel.0/@controlaction.6">
          <controlledprocess
              name="ADS-B"
              description="ADS-B"
              sendsInfo="//@systems.0/@systemModel.0/@subsystemModel.1/@systemModel.0/@otherinformation.0"
              sendsFeedbacks="//@systems.0/@systemModel.0/@subsystemModel.1/@systemModel.0/@feedback.0"/>
