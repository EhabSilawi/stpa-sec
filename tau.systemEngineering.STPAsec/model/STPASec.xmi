      <otherinformation
          name="Policy"
          description="Policy"
          controllerReceivers="//@systems.0/@systemModel.0/@controller.0"/>
      <otherinformation
          name="Certification information"
          description="Certification information"
          controllerSenders="//@systems.0/@systemModel.0/@controller.0"/>
      <otherinformation
          name="Airspace Transfer B"
          description="Airspace Transfer B"
          controllerSenders="//@systems.0/@systemModel.0/@controller.2"
          controllerReceivers="//@systems.0/@systemModel.0/@controller.1"/>
      <otherinformation
          name="Airspace Transfer A"
          description="Airspace Transfer A"
          controllerSenders="//@systems.0/@systemModel.0/@controller.1"
          controllerReceivers="//@systems.0/@systemModel.0/@controller.2"/>
      <otherinformation
          name="Attitude Information"
          description="Attitude Information"
          controllerReceivers="//@systems.0/@systemModel.0/@controller.4"
          subsystemSenders="//@systems.0/@systemModel.0/@subsystemModel.1/@systemModel.0"/>
      <otherinformation
          name="Attitude Information ITP"
          description="Attitude Information ITP"
          controllerReceivers="//@systems.0/@systemModel.0/@controller.3"
          subsystemSenders="//@systems.0/@systemModel.0/@subsystemModel.0/@systemModel.0"/>
