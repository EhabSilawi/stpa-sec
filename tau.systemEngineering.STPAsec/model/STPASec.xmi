<?xml version="1.0" encoding="UTF-8"?>
<STPAsec:STPASec
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:STPAsec="http://www.example.org/STPAsec">
  <systems name="NextGen In-Trail"
      description="NextGen In-Trail Procedure with new equipment (aviation)">
