  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="FeedbackFlow">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="feedbackSender" eType="#//ControlComponent"
        eOpposite="#//ControlComponent/sendsFeedback"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="feedbackReceiver" eType="#//ControlComponent"
        eOpposite="#//ControlComponent/receivesFeedback"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="feedback" eType="#//Feedback"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InfoFlow">
