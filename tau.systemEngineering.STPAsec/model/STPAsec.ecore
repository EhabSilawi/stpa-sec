<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="STPAsec" nsURI="http://www.example.org/STPAsec" nsPrefix="STPAsec">
  <eClassifiers xsi:type="ecore:EClass" name="STPASec">
    <eStructuralFeatures xsi:type="ecore:EReference" name="systems" upperBound="-1"
        eType="#//System" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="System">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="systemModel" upperBound="-1"
        eType="#//ControlStructure" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ControlStructure">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="modelGoal" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controlledprocess" upperBound="-1"
        eType="#//ControlledProcess" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controlaction" upperBound="-1"
        eType="#//ControlAction" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="feedback" upperBound="-1"
        eType="#//Feedback" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controller" upperBound="-1"
        eType="#//Controller" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subsystemModel" upperBound="-1"
        eType="#//System" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="otherinformation" upperBound="-1"
        eType="#//OtherInformation" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subsystemSenders" upperBound="-1"
        eType="#//Feedback" eOpposite="#//Feedback/sendsFeedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subsystemsReceivers" upperBound="-1"
        eType="#//Feedback" eOpposite="#//Feedback/receiversFeedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="swndsInfo" upperBound="-1"
        eType="#//OtherInformation" eOpposite="#//OtherInformation/subsystemSenders"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesInfo" upperBound="-1"
        eType="#//OtherInformation" eOpposite="#//OtherInformation/subsystemReceivers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sendsCommands" upperBound="-1"
        eType="#//ControlAction" eOpposite="#//ControlAction/subsystemSenders"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesCommands" upperBound="-1"
        eType="#//ControlAction" eOpposite="#//ControlAction/subsystemReceivers"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ControlStructureElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ControlledProcess" eSuperTypes="#//ControlStructureElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sendsInfo" upperBound="-1"
        eType="#//OtherInformation" eOpposite="#//OtherInformation/controledProcessSenders"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesInfo" upperBound="-1"
        eType="#//OtherInformation" eOpposite="#//OtherInformation/controledProcessReceivers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesCommands" upperBound="-1"
        eType="#//ControlAction" eOpposite="#//ControlAction/controledProccessReceivers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesFeedbacks" upperBound="-1"
        eType="#//Feedback" eOpposite="#//Feedback/controledProcessReceivers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sendsFeedbacks" upperBound="-1"
        eType="#//Feedback" eOpposite="#//Feedback/controledProcessSenders"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ControlAction" eSuperTypes="#//ControlStructureElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="controllerSenders" upperBound="-1"
        eType="#//Controller" eOpposite="#//Controller/sendsCommands"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controllerReceivers" upperBound="-1"
        eType="#//Controller" eOpposite="#//Controller/receivesCommands"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controledProccessReceivers"
        upperBound="-1" eType="#//ControlledProcess" eOpposite="#//ControlledProcess/receivesCommands"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subsystemSenders" upperBound="-1"
        eType="#//ControlStructure" eOpposite="#//ControlStructure/sendsCommands"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subsystemReceivers" upperBound="-1"
        eType="#//ControlStructure" eOpposite="#//ControlStructure/receivesCommands"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Feedback" eSuperTypes="#//ControlStructureElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="controllerSenders" upperBound="-1"
        eType="#//Controller" eOpposite="#//Controller/sendsFeedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controllerReceivers" upperBound="-1"
        eType="#//Controller" eOpposite="#//Controller/receivesFeedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controledProcessReceivers"
        upperBound="-1" eType="#//ControlledProcess" eOpposite="#//ControlledProcess/receivesFeedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controledProcessSenders"
        upperBound="-1" eType="#//ControlledProcess" eOpposite="#//ControlledProcess/sendsFeedbacks"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sendsFeedbacks" upperBound="-1"
        eType="#//ControlStructure" eOpposite="#//ControlStructure/subsystemSenders"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receiversFeedbacks" upperBound="-1"
        eType="#//ControlStructure" eOpposite="#//ControlStructure/subsystemsReceivers"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="OtherInformation" eSuperTypes="#//ControlStructureElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="controllerSenders" upperBound="-1"
        eType="#//Controller" eOpposite="#//Controller/sendsInfo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controledProcessSenders"
        upperBound="-1" eType="#//ControlledProcess" eOpposite="#//ControlledProcess/sendsInfo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controledProcessReceivers"
        upperBound="-1" eType="#//ControlledProcess" eOpposite="#//ControlledProcess/receivesInfo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="controllerReceivers" upperBound="-1"
        eType="#//Controller" eOpposite="#//Controller/receivesInfo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subsystemSenders" upperBound="-1"
        eType="#//ControlStructure" eOpposite="#//ControlStructure/swndsInfo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="subsystemReceivers" eType="#//ControlStructure"
        eOpposite="#//ControlStructure/receivesInfo"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Controller" eSuperTypes="#//ControlStructureElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="sendsFeedbacks" upperBound="-1"
        eType="#//Feedback" eOpposite="#//Feedback/controllerSenders"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sendsCommands" upperBound="-1"
        eType="#//ControlAction" eOpposite="#//ControlAction/controllerSenders"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sendsInfo" upperBound="-1"
        eType="#//OtherInformation" eOpposite="#//OtherInformation/controllerSenders"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesInfo" upperBound="-1"
        eType="#//OtherInformation" eOpposite="#//OtherInformation/controllerReceivers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesFeedbacks" upperBound="-1"
        eType="#//Feedback" eOpposite="#//Feedback/controllerReceivers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="receivesCommands" upperBound="-1"
        eType="#//ControlAction" eOpposite="#//ControlAction/controllerReceivers"/>
  </eClassifiers>
</ecore:EPackage>
