    <eStructuralFeatures xsi:type="ecore:EReference" name="feedbackflow" upperBound="-1"
        eType="#//FeedbackFlow" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="infoflow" upperBound="-1"
