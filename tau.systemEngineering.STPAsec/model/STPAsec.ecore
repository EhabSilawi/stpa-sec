    <eStructuralFeatures xsi:type="ecore:EReference" name="infoReceiver" eType="#//ControlComponent"
        eOpposite="#//ControlComponent/receivesInfo"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="infoSender" eType="#//ControlComponent"
        eOpposite="#//ControlComponent/sendsInfo"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Information">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
